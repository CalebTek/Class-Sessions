public static void MiniMaxSum(int[] arr)
        {
            int maxValue = 0;
            int minValue = 0;
            foreach (int i in arr)
            {
                maxValue = Math.Max(maxValue, i);
                minValue = Math.Min(minValue, i+1);
            }

            int smallestSum = 0;
            int largestSum = 0;

            foreach (int i in arr)
            {
                if (i != minValue)
                    smallestSum += i;
                if (i != maxValue)
                    largestSum += i;
            }

            Console.WriteLine(smallestSum);
            Console.WriteLine(largestSum);
        }

