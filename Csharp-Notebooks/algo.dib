static bool CheckUnique(string input)
    {
        bool[] seenChars = new bool[256];

        for (int i = 0; i < input.Length; i++)
        {
            int charCode = Convert.ToInt32(input[i]);
            if (seenChars[charCode])
            {
                return false;
            }
            seenChars[charCode] = true;
        }

        return true;
